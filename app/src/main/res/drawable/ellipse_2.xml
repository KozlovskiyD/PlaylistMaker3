<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:height="21dp"
        android:width="13dp"
        android:gravity="center">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/gray_light"/>
        </shape>
    </item>

    <item
        android:width="3dp"
        android:height="3dp"
        android:gravity="center">
        <shape
            android:shape="oval">
            <solid
                android:color="@color/black_icon"/>
        </shape>
    </item>
</layer-list>

